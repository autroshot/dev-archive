---
import ExternalLink from '@components/external-link.astro';
import StyledBlock from './styled-block.astro';

const { name, url } = Astro.props;

interface Props {
  name: string;
  url: string;
}
---

<StyledBlock title="원문" iconName="description">
  <ExternalLink href={url}>{name}</ExternalLink>
</StyledBlock>
